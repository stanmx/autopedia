<div class="page-header">
  <h2><%= @video.title %></h2>
</div>   
<p>
	<b>Fecha de Publicación:</b>
	<%= l @video.created_at, :formats => :long %>
<p>
	<b>Descripción:</b>
	<%= @video.description %>
</p>

<%= @video.video.html_safe %>

<div class="form-actions">
  <%= link_to 'Editar', edit_video_path(@video), :class => "btn" %>
  <%= link_to 'Regresar', videos_path, :class => "btn" %>
</div>